                        }


                        function loadCountryCode() {
                            var selectedCountry = $('#txtPax1_Ctry :selected').text();
                            $(".mobileNumber input").intlTelInput("destroy");
                            if (selectedCountry == "Canada") {
                                //alert(selectedCountry);
                                $(".mobileNumber input").intlTelInput({ onlyCountries: ["ca"], selectCountry: "ca" });
                            } else {
                                $(".mobileNumber input").intlTelInput({
                                    initialCountry: "auto",
                                    separateDialCode: true,
                                    setCountry: initalizeCountry(),
                                    utilsScript: "javascript/intlTelInput/utils.js"

                                });
                            }

