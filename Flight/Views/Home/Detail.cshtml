    <div id="wrap">
        <style>
            #xuathoadon {
                /*	background: #F7FFF2;
                padding: 1rem;*/
                opacity: 1;
                max-height: 1000px;
                overflow: hidden;
                transition: all 1s;
            }

                #xuathoadon.an {
                    company-taxid opacity: 0;
                    max-height: 0;
                }

            .passenger_name {
                height: 27px;
