            }
        </style>
        <div class="customer-information">

            <div class="review">
                <style>
                    .block-title {
                        font-size: 13px;
                        padding: 8px 0 5px;
                    }

                    .block-content {
                        border: 1px solid gainsboro;
                        padding: 6px 8px 0;
                    }

                    .col-xs-3 {
                        float: left;
                        padding: 6px 0;
                        width: 23%;
                    }

                    .row {
                        overflow: auto;
                    }
                </style>

                <div class="block block-price-detail">
                    <div class="block-header">
                        <h4 class="block-title" style="font-weight: bold;">Chi tiết Giá</h4>
                    </div>
                    <div class="block-content">
                        <div class="table-price">
                            <div class="row table-price-head">
                                <div class="col-xs-3">
                                    <span style="font-weight: bold;">Hành khách</span>
                                </div>
                                <div class="col-xs-3">
                                    <span style="font-weight: bold;">Giá vé</span>
                                </div>
                                <div class="col-xs-3">
                                    <span style="font-weight: bold;">Thuế &amp; Phí</span>
                                </div>
                                <div class="col-xs-3">
                                    <span style="font-weight: bold;">Tổng cộng</span>
                                </div>
                            </div>
                            @if (@Model.adultNo > 0)
                            {
                                <div class="row">
                                    <div class="col-xs-3">
                                        <span>@Model.adultNo x </span><span>Người lớn</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.cb.Gia VNĐ</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.cb.Thue VNĐ</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.tongGiaVeNguoiLon VNĐ</span>
                                    </div>
                                </div>
                            }
                            @if (@Model.childNo > 0)
                            {
                                <div class="row">
                                    <div class="col-xs-3">
                                        <span>@Model.childNo x </span><span>Trẻ em </span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.cb.GiaTreEm VNĐ</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.cb.ThueTreEm VNĐ</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.tongGiaVetreEm VNĐ</span>
                                    </div>
                                </div>
                            }
                            @if (@Model.infantNo > 0)
                            {
                                <div class="row">
                                    <div class="col-xs-3">
                                        <span>@Model.childNo x </span><span>Trẻ Sơ Sinh </span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.cb.GiaVeTreSoSinh VNĐ</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">0 VNĐ</span>
                                    </div>
                                    <div class="col-xs-3">
                                        <span class="currency">@Model.tongGiaveSoSinh VNĐ</span>
                                    </div>
                                </div>
                            }


                        </div>


